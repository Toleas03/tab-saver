{
    "manifest_version": 3,
    "name": "Tab Saver",
    "version": "1.0",
    "description": "Save and manage your browser tabs",
    "host_permissions": [
        "https://developer.chrome.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}